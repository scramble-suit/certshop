{
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}"
        },
        {
            "name": "Create CA",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["-debug", "-overwrite", "ca", "ca"],
            "showLog": true
        },
        {
            "name": "Create ICA",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["ica", "-maxPathLength=0", "ca/ica"],
            "showLog": true
        },
        {
            "name": "Create server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["-overwrite", "server", "-san=127.0.0.1,localhost,::1", "ca/server"],
            "showLog": true
        },
        {
            "name": "Create CSR",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["csr", "-dn=/CN=tossme"],
            "showLog": true
        },
        {
            "name": "export",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["export", "-format=p12", "-passOut=1234", "ca/ica1/ica2/server"],
            "showLog": true
        }
    ]
}