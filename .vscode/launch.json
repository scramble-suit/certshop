{
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}"
        },
        {
            "name": "Create CA",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["-debug", "-overwrite", "ca", "-csrFile=tossme/tossme-csr.pem", "ca"],
            "showLog": true
        },
        {
            "name": "Create ICA",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["ica", "-maxPathLength=0", "ca/ica"],
            "showLog": true
        },
        {
            "name": "Create CSR",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["csr", "-dn=/CN=ACME"],
            "showLog": true
        },
        {
            "name": "Create Signature",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["-overwrite", "-debug", "signature", "ca/ica/signature"],
            "showLog": true
        }
    ]
}